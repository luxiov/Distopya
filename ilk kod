const Discord = require('discord.js');
const client = new Discord.Client();

client.on('ready', () => {
  console.log(`Logged in as ${client.user.tag}!`);
});

client.on('message', msg => {
  if (msg.content === 'ping') {
    msg.reply('Pong!');
  }
});

client.login('MTE0NTg0NzA4NDg1MTkyOTA4OQ.G8fbGW.52eUbWaOera8awot-UCt7N8YguQvQpOpwuJRvE');
